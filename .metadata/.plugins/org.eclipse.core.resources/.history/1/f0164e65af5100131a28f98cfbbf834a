package Dice;

public class DiceTest {
    public static void main(String[] args) {
        Dice dice = new Dice();
        for(int i = 0; i < 10; i++) {
        	System.out.println(dice.roll2()); // 4
        }
        if(i == 1) {
        	//いかさま
        	dice.roll();
        }
        if(i == 2) {
        	//正規
        	dice.roll2();
        }
        
    }
}